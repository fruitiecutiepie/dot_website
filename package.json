{
  "publisher": "Selfprogrammed",
  "name": "selfprogrammed-browser",
  "displayName": "Selfprogrammed Browser",
  "version": "0.3.9",
  "description": "Embedded browser in VS Code.",
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.83.0"
  },
  "activationEvents": [
    "onOpenExternalUri:http",
    "onOpenExternalUri:https",
    "onDebugInitialConfigurations",
    "onDebug",
    "onFileSystem:vsls"
  ],
  "contributes": {
    "commands": [
      {
        "category": "Selfprogrammed Browser",
        "command": "selfprogrammed.browser.open",
        "title": "Open New Website"
      },
      {
        "category": "Selfprogrammed Browser",
        "command": "selfprogrammed.browser.openActiveFile",
        "title": "Open Active File in Preview"
      },
      {
        "category": "Selfprogrammed Browser",
        "command": "selfprogrammed.browser.controls.refresh",
        "title": "Refresh Page",
        "icon": "$(refresh)"
      },
      {
        "category": "Selfprogrammed Browser",
        "command": "selfprogrammed.browser.controls.external",
        "title": "Open page with system browser",
        "icon": "$(link-external)"
      },
      {
        "category": "Selfprogrammed Browser",
        "command": "selfprogrammed.browser.controls.debug",
        "title": "Open DevTools",
        "icon": "$(tools)"
      },
      {
        "category": "Selfprogrammed Browser",
        "command": "selfprogrammed.browser.controls.openSourceDocument",
        "title": "Open .website Source File",
        "icon": "$(file-code)"
      }
    ],
    "configuration": {
      "title": "Selfprogrammed Browser",
      "type": "object",
      "properties": {
        "selfprogrammed.browser.startUrl": {
          "description": "The default start url for new Selfprogrammed Browser instances",
          "type": "string"
        },
        "selfprogrammed.browser.chromeExecutable": {
          "description": "The full path to the executable, including the complete filename of the executable",
          "type": "string"
        },
        "selfprogrammed.browser.verbose": {
          "default": false,
          "description": "Toggles verbose logging",
          "type": "boolean"
        },
        "selfprogrammed.browser.debugHost": {
          "default": "localhost",
          "description": "Host name for debug",
          "type": "string"
        },
        "selfprogrammed.browser.debugPort": {
          "default": 9222,
          "description": "Port number for debug, when occupied, it will try to find another one by self bumpping.",
          "type": "number"
        },
        "selfprogrammed.browser.format": {
          "default": "png",
          "enum": [
            "png",
            "jpeg"
          ],
          "description": "The type of image used in rendering preview.",
          "type": "string"
        },
        "selfprogrammed.browser.ignoreHttpsErrors": {
          "default": true,
          "description": "Ignore HTTPS errors if you are using self-signed SSL certificates",
          "type": "boolean"
        },
        "selfprogrammed.browser.quality": {
          "default": 100,
          "description": "Image quality of screencasting",
          "type": "number"
        },
        "selfprogrammed.browser.everyNthFrame": {
          "default": 1,
          "description": "Skip for frames of screencasting",
          "type": "number"
        },
        "selfprogrammed.browser.localFileAutoReload": {
          "default": true,
          "description": "Automatically reload page on local file changes",
          "type": "boolean"
        },
        "selfprogrammed.browser.storeUserData": {
          "default": true,
          "description": "Store cookies, localStorage, etc. on disk, so that you don't lose them when you close session. This will help you not get logged out.",
          "type": "boolean"
        },
        "selfprogrammed.browser.proxy": {
          "description": "Add proxy parameters during initial loading of the browser.",
          "type": "string"
        },
        "selfprogrammed.browser.otherArgs": {
          "description": "Add other parameters during initial loading of the browser.",
          "type": "string"
        }
      }
    },
    "customEditors": [
      {
        "viewType": "selfprogrammed.browser.editor",
        "displayName": "Website",
        "selector": [
          {
            "filenamePattern": "*.website"
          }
        ],
        "priority": "default"
      }
    ],
    "debuggers": [
      {
        "type": "selfprogrammed.browser",
        "label": "Selfprogrammed Browser",
        "configurationSnippets": [
          {
            "label": "Selfprogrammed Browser: Launch",
            "description": "Launch Selfprogrammed Browser to localhost",
            "body": {
              "type": "selfprogrammed.browser",
              "request": "launch",
              "name": "Selfprogrammed Browser: Launch",
              "url": "http://localhost:3000"
            }
          },
          {
            "label": "Selfprogrammed Browser: Attach",
            "description": "Attach to open Selfprogrammed Browser instances",
            "body": {
              "type": "selfprogrammed.browser",
              "request": "attach",
              "name": "Selfprogrammed Browser: Attach"
            }
          }
        ],
        "configurationAttributes": {
          "launch": {
            "properties": {
              "url": {
                "type": "string",
                "description": "Absolute url to launch",
                "default": "http://localhost:3000"
              },
              "webRoot": {
                "type": "string",
                "description": "This specifies the workspace absolute path to the webserver root. Used to resolve paths like `/app.js` to files on disk. Shorthand for a pathMapping for \"/\"",
                "default": "${workspaceFolder}"
              },
              "pathMapping": {
                "type": "object",
                "description": "A mapping of URLs/paths to local folders, to resolve scripts in Chrome to scripts on disk",
                "default": {
                  "/": "${workspaceFolder}"
                }
              },
              "trace": {
                "type": [
                  "boolean",
                  "string"
                ],
                "enum": [
                  "verbose",
                  true
                ],
                "default": true,
                "description": "When 'true', the debugger will log tracing info to a file. When 'verbose', it will also show logs in the console."
              },
              "sourceMapPathOverrides": {
                "type": "object",
                "description": "A set of mappings for rewriting the locations of source files from what the sourcemap says, to their locations on disk. See README for details.",
                "default": {
                  "webpack:///./*": "${webRoot}/*",
                  "webpack:///src/*": "${webRoot}/*",
                  "webpack:///*": "*",
                  "webpack:///./~/*": "${webRoot}/node_modules/*",
                  "meteor://ðŸ’»app/*": "${webRoot}/*"
                }
              }
            }
          },
          "attach": {
            "properties": {
              "urlFilter": {
                "type": "string",
                "description": "Will search for a page with this url and attach to it, if found. Can have * wildcards.",
                "default": ""
              },
              "webRoot": {
                "type": "string",
                "description": "This specifies the workspace absolute path to the webserver root. Used to resolve paths like `/app.js` to files on disk. Shorthand for a pathMapping for \"/\"",
                "default": "${workspaceFolder}"
              },
              "pathMapping": {
                "type": "object",
                "description": "A mapping of URLs/paths to local folders, to resolve scripts in Chrome to scripts on disk",
                "default": {
                  "/": "${workspaceFolder}"
                }
              },
              "trace": {
                "type": [
                  "boolean",
                  "string"
                ],
                "enum": [
                  "verbose",
                  true
                ],
                "default": true,
                "description": "When 'true', the debugger will log tracing info to a file. When 'verbose', it will also show logs in the console."
              },
              "sourceMapPathOverrides": {
                "type": "object",
                "description": "A set of mappings for rewriting the locations of source files from what the sourcemap says, to their locations on disk. See README for details.",
                "default": {
                  "webpack:///./*": "${webRoot}/*",
                  "webpack:///src/*": "${webRoot}/*",
                  "webpack:///*": "*",
                  "webpack:///./~/*": "${webRoot}/node_modules/*",
                  "meteor://ðŸ’»app/*": "${webRoot}/*"
                }
              }
            }
          }
        }
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "!explorerResourceIsFolder && resourceExtname == .website",
          "command": "selfprogrammed.browser.controls.openSourceDocument",
          "group": "selfprogrammed.browser"
        }
      ],
      "editor/title": [
        {
          "when": "resourceScheme == webview-panel && selfprogrammed.browser-active",
          "command": "selfprogrammed.browser.controls.external",
          "group": "navigation"
        },
        {
          "when": "resourceScheme == webview-panel || resourceScheme == file && selfprogrammed.browser-active && !selfprogrammed.browser-debug-active",
          "command": "selfprogrammed.browser.controls.debug",
          "group": "navigation"
        },
        {
          "when": "resourceExtname == .website && selfprogrammed.browser-active && activeEditor != activeCustomEditorId",
          "command": "selfprogrammed.browser.controls.openSourceDocument",
          "group": "navigation"
        }
      ],
      "commandPalette": [
        {
          "command": "selfprogrammed.browser.controls.external",
          "when": "false"
        },
        {
          "command": "selfprogrammed.browser.controls.debug",
          "when": "false"
        },
        {
          "command": "selfprogrammed.browser.controls.openSourceDocument",
          "when": "false"
        }
      ],
      "view/item/context": [
        {
          "when": "view == selfprogrammed.file-explorer.tree_view && viewItem == file",
          "command": "selfprogrammed.browser.controls.openSourceDocument",
          "group": "selfprogrammed.browser"
        }
      ]
    },
    "walkthroughs": [
      {
        "id": "walkthrough",
        "title": "Get Started with Selfprogrammed Browser",
        "description": "Reduce context switching and stay focused in a single environment with embedded browser.",
        "steps": [
          {
            "id": "selfprogrammed.browser.walkthrough.step_1",
            "title": "Optimize your browser workflow",
            "description": "Manage your web resources in a streamlined way. You're only one click away from optimizing your DX.\n[Create a .website File](command:selfprogrammed.browser.walkthrough.step_1)",
            "media": {
              "markdown": "./assets/walkthrough/step_1.md"
            }
          },
          {
            "id": "selfprogrammed.browser.walkthrough.step_2",
            "title": "Open a new website",
            "description": "Quickly launch a new tab with your desired website by opening the command palette.",
            "media": {
              "markdown": "./assets/walkthrough/step_2.md"
            }
          },
          {
            "id": "selfprogrammed.browser.walkthrough.step_3",
            "title": "Preview your websites",
            "description": "See your website hot reload in VS Code.",
            "media": {
              "markdown": "./assets/walkthrough/step_3.md"
            }
          },
          {
            "id": "selfprogrammed.browser.walkthrough.step_4",
            "title": "Debug websites",
            "description": "Use DevTools within VS Code for efficient website inspection and debugging.",
            "media": {
              "markdown": "./assets/walkthrough/step_4.md"
            }
          }
        ]
      }
    ]
  },
  "scripts": {
    "build:dev": "vite build && pnpm build:ts",
    "publish": "vsce publish --no-dependencies",
    "package": "vsce package --no-dependencies --allow-missing-repository",
    "build": "rimraf dist && vite build && pnpm build:ts --minify",
    "build:ts": "tsup src/extension.ts --external=vscode -d dist",
    "lint": "eslint .",
    "release": "bumpp",
    "install_extension": "pnpm build && pnpm package && node install_extension.js"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.0.0-beta.27",
    "@chiragrupani/karma-chromium-edge-launcher": "^2.3.1",
    "@types/karma-chrome-launcher": "^3.1.2",
    "@types/lodash": "^4.14.199",
    "@types/node": "20.8.6",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^18.2.13",
    "@types/vscode": "1.83.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vscode/vsce": "^2.21.1",
    "browser-viewport-device-descriptions": "^1.1.0",
    "bumpp": "^9.2.0",
    "conventional-github-releaser": "^3.1.5",
    "element-to-source": "^1.0.1",
    "eslint": "^8.51.0",
    "event-emitter-enhancer": "^2.0.0",
    "eventemitter2": "^6.4.9",
    "find-up": "^6.3.0",
    "karma-chrome-launcher": "^3.2.0",
    "lodash": "^4.17.21",
    "ovsx": "^0.8.3",
    "puppeteer-core": "^21.3.8",
    "re-resizable": "^6.9.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11"
  },
  "preview": true,
  "extensionKind": [
    "ui",
    "workspace"
  ]
}
